{
  "name": "prisma",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "db:generate": "dotenv -e local.env npx prisma generate",
    "db:migrate": "dotenv -e local.env npx prisma migrate dev",
    "db:reset": "dotenv -e local.env npx prisma migrate reset",
    "db:studio": "dotenv -e local.env npx prisma studio",
    "db:push": "dotenv -e local.env npx prisma db push",
    "db:dev": "npm run db:generate && npm run db:studio",
    "db:dev:sync": "npm run db:push && npm run db:generate",
    "dev": "cross-env NODE_ENV=development dotenv -e local.env concurrently \"npm:codegen:watch\" \"nodemon\"",
    "test": "jest --watch",
    "seed": "cross-env SEED_TYPE=all tsx prisma/seed/index.ts",
    "seed:custom": "cross-env tsx prisma/seed/index.ts",
    "db:seed": "npm run db:reset && npm run seed",
    "build": "npm run codegen && tsc",
    "start": "node dist/index.js",
    "codegen": "graphql-codegen --config codegen.config.ts",
    "codegen:watch": "graphql-codegen --watch --config codegen.config.ts"
  },
  "dependencies": {
    "@prisma/client": "6.16.2",
    "bcryptjs": "^3.0.2",
    "graphql": "16.11.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "6.0.6",
    "graphql-yoga": "^5.15.2",
    "jsonwebtoken": "^9.0.2",
    "pino": "9.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "6.0.0",
    "@graphql-codegen/typescript": "5.0.0",
    "@graphql-codegen/typescript-resolvers": "5.0.0",
    "@graphql-tools/graphql-file-loader": "8.1.1",
    "@graphql-tools/load-files": "7.0.1",
    "@parcel/watcher": "2.5.1",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.17.0",
    "concurrently": "9.2.1",
    "cross-env": "10.0.0",
    "dataloader": "2.2.3",
    "dotenv-cli": "10.0.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "pino-pretty": "13.1.1",
    "prisma": "6.16.2",
    "ts-jest": "^29.4.4",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
